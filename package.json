    "eslint": "^6.8.0",
    "mocha": "^7.1.1"
  },
  "resolutions": {
    "lodash": "^4.17.15",
    "yargs-parser": "^18.1.3",
    "node-forge": "^0.10.0",
    "core-js": "^3.6.5"
  }
}
