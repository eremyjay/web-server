    "jsonwebtoken": "^8.5.1",
    "lodash.assignin": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.27",
    "node-base64-image": "^1.0.7",
    "node-machine-id": "^1.1.12",
    "node-turn": "0.0.6",
    "npm-force-resolutions": "0.0.3",
    "pem": "^1.14.4",
    "postman-request": "^2.88.1-postman.22",
    "rate-limiter-flexible": "^2.1.13",
    "spdy": "^4.0.1",
