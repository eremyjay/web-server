{
  "name": "@eremyjay/web-server",
  "description": "A web server based on feathers with multiple features including mongoose integration, redis support, choice of web server, built in user authentication, built in services management and heartbeat.",
  "version": "1.0.0",
  "homepage": "https://localhost",
  "main": "server",
  "keywords": [
    "web", "server", "feathers", "mongoose", "redis", "spdy", "https", "authentication", "c2"
  ],
  "author": {
    "name": "Jeremy de Oliveira-Kumar",
    "email": "web-server@localhost"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "test/"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "build": "npm build",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint server/. test/. --config .eslintrc.json",
    "start": "node server/",
    "preinstall": "# npx npm-force-resolutions",
    "mocha": "mocha test/ --recursive"
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.2",
    "@feathersjs/authentication-local": "^4.5.2",
    "@feathersjs/authentication-oauth": "^4.5.2",
    "@feathersjs/configuration": "^4.5.2",
    "@feathersjs/errors": "^4.5.2",
    "@feathersjs/express": "^4.5.2",
    "@feathersjs/feathers": "^4.5.2",
    "@google-cloud/logging-winston": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto-random-string": "^3.2.0",
    "dompurify": "^2.0.8",
    "express-session": "^1.17.1",
    "express-sticky-cluster": "git+https://github.com/interactdo/express-sticky-cluster.git",
    "feathers-mongoose": "^8.3.0",
    "feathers-redis-cache": "git+https://github.com/interactdo/feathers-redis-cache.git",
    "get-random-values": "^1.2.0",
    "helmet": "^3.21.3",
    "ioredis": "^4.16.0",
    "ip6addr": "^0.2.3",
